{
  "name": "@groccia/medusa-postcode-plugin",
  "version": "0.0.1",
  "description": "MedusaJS plugin for syncing data from postcode dataset",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/groccia/medusa-fi-postcode-plugin.git"
  },
  "author": "Medusa (https://medusajs.com)",
  "license": "MIT",
  "keywords": [
    "medusa-plugin-other",
    "medusa"
  ],
  "scripts": {
    "clean": "cross-env ./node_modules/.bin/rimraf dist",
    "build": "cross-env npm run clean && npm run build:server",
    "build:server": "cross-env npm run clean && tsc -p tsconfig.server.json",
    "watch": "cross-env tsc --watch",
    "test": "cross-env jest",
    "start": "cross-env npm run build && medusa start",
    "start:custom": "cross-env npm run build && node --preserve-symlinks --trace-warnings index.js",
    "dev": "cross-env npm run build:server && medusa develop",
    "prepare": "cross-env NODE_ENV=production npm run build"
  },
  "dependencies": {
    "@medusajs/file-local": "^1.0.3",
    "@tanstack/react-query": "4.22.0",
    "adm-zip": "^0.5.10",
    "axios": "^1.6.7",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "16.4.5",
    "express": "^4.17.2",
    "iconv-lite": "^0.6.3",
    "linebyline": "^1.3.0",
    "medusa-interfaces": "^1.3.8",
    "prism-react-renderer": "^2.0.4",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/preset-typescript": "^7.21.4",
    "@jest/globals": "^29.7.0",
    "@medusajs/medusa": "^1.20.2",
    "@medusajs/medusa-cli": "^1.3.21",
    "@types/adm-zip": "^0.5.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "babel-preset-medusa-package": "^1.1.19",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "jest": "^27.3.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^4.5.2"
  },
  "overrides": {
    "@medusajs/admin-ui": {
      "@medusajs/ui-preset": "^1.1.2"
    }
  },
  "resolutions": {
    "@medusajs/ui-preset": "^1.1.2"
  },
  "peerDependencies": {
    "@medusajs/medusa": "1.20.2"
  }
}
